{
  "specFlow": {
    "bindingCulture": {
      "name": "en"
    },
    "unitTestProvider": {
      "name": "NUnit"
    },
    "runtime": {
      "obtainDependenciesFromNuGet": true,
      "parallelExecution": {
        "testThreadCount": 4, 
        "syncContext": false
      }
    },
    "trace": {
      "level": "Verbose",
      "traceSuccessfulSteps": true,
      "traceTimings": true,
      "minTracedDuration": "00:00:00.1",
      "traceListener": "SharpAutomation.Helpers.CorrelatedTracer"
    },
    "stepAssemblies": [
      {"assembly": "Allure.SpecFlowPlugin"}
    ]
  }
}
