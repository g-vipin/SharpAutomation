{
    "AppSettings": {
        "Browser": "Chrome",
        "Headless": "False",
        "Environment": "Dev",
        "BaseUrl": "https://www.saucedemo.com/"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "logs/log-.log",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 20
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    }
}